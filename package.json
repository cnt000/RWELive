{
  "name": "rwe-live-companion",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": "24.x",
    "pnpm": "10.x"
  },
  "scripts": {
    "preinstall": "node -e \"const ua = process.env.npm_config_user_agent || ''; if (!ua.includes('pnpm')) { console.error('This project requires pnpm. Please run: pnpm install'); process.exit(1); }\"",
    "build": "vite build",
    "dev": "vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "pnpm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && pnpm run clean && pnpm run build && wrangler deploy",
    "generate": "rw-scripts ensure-env && wrangler types --include-runtime false",
    "check": "pnpm run generate && pnpm run types",
    "types": "tsc"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-server-dom-webpack": "^19.2.4",
    "rwsdk": "1.0.0-beta.53"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.25.0",
    "@cloudflare/workers-types": "^4.20260213.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.17.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wrangler": "^4.65.0"
  },
  "volta": {
    "node": "24.13.1"
  }
}
